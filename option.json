{
	"manager": "npm",
	"managerArray":
	{
		"npm":
		[
			"install",
			"--no-package-lock"
		],
		"yarn":
		[
			 "--no-lockfile"
		]
	},
	"path": "package.json",
	"ignoreArray":
	[
		"dependencies",
		"devDependencies"
	],
	"targetArray":
	[
		"dependencies",
		"devDependencies",
		"peerDependencies"
	],
	"prefix": "__",
	"indent": 4
}
